{
  "name": "amplesauce-firebase-functions",
  "private": true,
  "scripts": {
    "login": "firebase login",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy-functions": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "get-config": "firebase functions:config:get > .runtimeconfig.json",
    "emulators": "cd .. && npm run emulators"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@genkit-ai/googleai": "^1.18.0",
    "dotenv": "^17.2.3",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "firebase-functions-rate-limiter": "^3.9.1",
    "firebase-tools": "^14.8.0",
    "genkit": "^1.18.0",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.1",
    "typescript": "^5.8.3"
  }
}
